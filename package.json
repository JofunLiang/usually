  "author": "jofun",
  "repository": {
    "type": "git",
    "url": "git@github.com:JofunLiang/usuallyjs.git"
  },
  "homepage": "https://jofunliang.github.io/usuallyjs/",
  "keywords": [
    "usuallyjs",
    "utils",
    "utils-library",
    "javascript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JofunLiang/usuallyjs/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/runtime": "^7.2.0",
    "@babel/runtime-corejs2": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "docdash": "^1.0.1",
